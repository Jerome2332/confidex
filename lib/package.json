{
  "name": "@confidex/sdk",
  "version": "0.1.0",
  "description": "Confidex SDK - Encryption and client utilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./encryption": {
      "import": "./dist/encryption.mjs",
      "require": "./dist/encryption.js",
      "types": "./dist/encryption.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "test": "vitest"
  },
  "dependencies": {
    "@arcium-hq/client": "^0.6.3",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "@solana/web3.js": "^1.95.0",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "@solana/web3.js": ">=1.90.0"
  }
}
